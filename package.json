	"private": true,
	"license": "MIT",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "husky && svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"deps": "pnpm dlx taze@latest -Ilw",
		"deps:major": "pnpm dlx taze@latest major -Ilw",
		"ui": "pnpm dlx shadcn-svelte@latest"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@ai-sdk/google": "^2.0.44",
		"@ai-sdk/svelte": "^3.0.108",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.556.0",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"ai": "^5.0.108",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"mode-watcher": "^1.1.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.45.7",
		"svelte-check": "^4.3.4",
		"svelte-highlight": "^7.9.0",
		"svelte-sonner": "^1.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.49.0",
		"vite": "^7.2.7"
